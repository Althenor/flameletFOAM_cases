FoamFile
{
    version     2.0;
    format      ascii;
    class       volVectorField;
    location    "0";
    object      U;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 -1 0 0 0 0];

internalField   uniform (0 0 4.8);

boundaryField
{
    AIR1
    {
        type            timeVaryingMappedFixedValue;
        setAverage      false;
        offset          (0 0 0);
        mapMethod       nearest;
        perturb         0;
    }
    AIR2
    {
        type            timeVaryingMappedFixedValue;
        setAverage      false;
        offset          (0 0 0);
        mapMethod       nearest;
        perturb         0;
    }
    AIR3
    {
        type            timeVaryingMappedFixedValue;
        setAverage      false;
        offset          (0 0 0);
        mapMethod       nearest;
        perturb         0;
    }
    AIR4
    {
        type            timeVaryingMappedFixedValue;
        setAverage      false;
        offset          (0 0 0);
        mapMethod       nearest;
        perturb         0;
    }
    AIR
    {
        type            fixedValue;
        value           uniform (0 0 0.65);
    }
    WALLTUBE
    {
        type            fixedValue;
        value           uniform (0 0 0);
    }
    WALLOUT
    {
        type            fixedValue;
        value           uniform (0 0 0);
    }
    EDGE
    {
        type            fixedValue;
        value           uniform (0 0 0);
    }
    BOTTOM
    {
        type            pressureInletOutletVelocity;
        value           uniform (0 0 0);
    }
    OUT
    {
        type            pressureInletOutletVelocity;
        value           uniform (0 0 0);
    }
    ATOM
    {
        type            pressureInletOutletVelocity;
        value           uniform (0 0 0);
    }
    FUEL
    {
        type            timeVaryingMappedFixedValue;
        setAverage      false;
        offset          (0 0 0);
        mapMethod       nearest;
        perturb         0;
    }
}

