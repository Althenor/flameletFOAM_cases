FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      fvSchemes;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

ddtSchemes
{
    default         @TIMESCHEME@;
}

gradSchemes
{
    default         @GRAD@;
//    grad(U)         @GRAD@;
//    grad(p)         @GRAD@;
//    grad(Z)         @GRAD@;
}

divSchemes
{
    default         none;
    div(phi,U)      Gauss @DIVPHIU@;
    div(phi,Z)      Gauss @DIVPHIZ@;
    div((muEff*dev2(T(grad(U))))) Gauss linear;
}

laplacianSchemes
{
    default         Gauss @LAPS@ corrected;
//    laplacian(muEff,U)         Gauss @LAPS@ corrected;
//    laplacian(rhorAUf,p)       Gauss @LAPS@ corrected;
//    laplacian(DZEff,Z)         Gauss @LAPS@ corrected;
}

interpolationSchemes
{
    default         linear;
}

snGradSchemes
{
    default         corrected;
}

fluxRequired
{
    default         no;
    p;
}


